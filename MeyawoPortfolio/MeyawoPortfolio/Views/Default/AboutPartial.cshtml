@model List<MeyawoPortfolio.Models.TblAbout>
@foreach (var x in Model)
{
    <section class="section pt-0" id="about">
        <!-- container -->
        <div class="container text-center">
            <!-- about wrapper -->
            <div class="about">
                <div class="about-img-holder">
                    <img src=@x.ImageUrl class="about-img"
                         alt="Download free bootstrap 4 landing page, free boootstrap 4 templates, Download free bootstrap 4.1 landing page, free boootstrap 4.1.1 templates, meyawo Landing page">
                </div>
                <div class="about-caption">
                    <p class="section-subtitle">@x.Title</p>
                    <h2 class="section-title mb-3">@x.Header</h2>
                    <p>
                        @x.Description
                    </p>
                    <a href="~/Template/Cv.pdf" class="btn-rounded btn btn-outline-primary mt-4" download="Cv.pdf">Cv'mi İndir</a>
                </div>
            </div><!-- end of about wrapper -->
        </div><!-- end of container -->
    </section>
}
